{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.py",
      "use": "@vercel/python"
    },
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/api/hello",
      "dest": "/api/hello"
    },
    {
      "src": "/api/birth-chart",
      "dest": "/api/birth-chart"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    }
  ],
  "public": true
} 